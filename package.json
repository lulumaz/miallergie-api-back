{
  "name": "miallergie-api-back",
  "version": "1.0.8",
  "description": "miallergie-api-back",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t miallergie-api-back .",
    "docker:run": "docker run -p 3000:3000 -d miallergie-api-back",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "start/watch": "nodemon -r source-map-support/register .",
    "prepublishOnly": "npm run test",
    "initBase": "node ./dist/init"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@loopback/authentication": "^4.2.2",
    "@loopback/authentication-passport": "^2.1.2",
    "@loopback/authorization": "^0.5.7",
    "@loopback/boot": "^2.1.2",
    "@loopback/context": "^3.6.0",
    "@loopback/core": "^2.4.2",
    "@loopback/extension-health": "^0.4.2",
    "@loopback/openapi-v3": "^3.3.0",
    "@loopback/repository": "^2.3.0",
    "@loopback/rest": "^3.3.2",
    "@loopback/rest-explorer": "^2.1.2",
    "@loopback/service-proxy": "^2.1.2",
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/multer": "^1.4.2",
    "bcrypt": "^3.0.7",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "loopback-connector-mongodb": "^4.2.0",
    "minimist": "^1.2.3",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^5.3.0",
    "@loopback/eslint-config": "^6.0.5",
    "@loopback/testlab": "^3.1.2",
    "@types/node": "^10.17.21",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.3.0",
    "source-map-support": "^0.5.19",
    "typescript": "~3.8.3"
  }
}
