{
  "name": "miallergie-api-back",
  "version": "1.0.0",
  "description": "miallergie-api-back",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t miallergie-api-back .",
    "docker:run": "docker run -p 3000:3000 -d miallergie-api-back",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "start/watch": "nodemon -r source-map-support/register .",
    "prepublishOnly": "npm run test",
    "initBase": "node ./dist/init"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@loopback/authentication": "^3.3.2",
    "@loopback/authentication-passport": "^1.1.2",
    "@loopback/boot": "^1.7.3",
    "@loopback/context": "^2.1.0",
    "@loopback/core": "^1.12.3",
    "@loopback/extension-health": "^0.2.16",
    "@loopback/openapi-v3": "^1.13.0",
    "@loopback/repository": "^1.19.0",
    "@loopback/rest": "^1.26.1",
    "@loopback/rest-explorer": "^1.4.9",
    "@loopback/service-proxy": "^1.3.16",
    "@types/bcrypt": "^3.0.0",
    "bcrypt": "^3.0.7",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-mongodb": "^4.2.0",
    "minimist": "^1.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^3.1.1",
    "@loopback/eslint-config": "^5.0.3",
    "@loopback/testlab": "^1.10.3",
    "@types/node": "^10.17.14",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.2.2",
    "source-map-support": "^0.5.16",
    "typescript": "~3.7.5"
  }
}
